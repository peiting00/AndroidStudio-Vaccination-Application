<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.group14_vaccinationapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_vaccine"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.Group14_VaccinationApp">
        <activity
            android:name=".MainActivity"
            android:label="@string/title_activity_main"
            android:theme="@style/Theme.Group14_VaccinationApp.NoActionBar"></activity>

        <activity
            android:name=".CovidWebsite"
            android:label="CovidWebsite"></activity>

        <activity
            android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>