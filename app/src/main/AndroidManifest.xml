<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.group14_vaccinationapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_vaccine"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.Group14_VaccinationApp">
        <activity android:name=".Quiz"></activity>
        <activity android:name=".DeclarationVaccine" />
        android:theme="@style/Theme.Group14_VaccinationApp" >
        <activity android:name=".Registration" >
        </activity>

        <activity
            android:name=".MainActivity"
            android:label="@string/title_activity_main"
            android:theme="@style/Theme.Group14_VaccinationApp.NoActionBar" />
        <activity android:name=".CovidWebsite" />
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".VaccinationMenu"
            android:label="@string/vaccination" />
        <activity android:name=".SelectVaccine" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>